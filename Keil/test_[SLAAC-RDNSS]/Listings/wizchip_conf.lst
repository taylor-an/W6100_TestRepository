S/**
S * @ingroup DATA_TYPE
S * @brief Callback function set for @ref _WIZCHIP_
S * @details @ref __WIZCHIP_T__ is a structure type to provide the callback function set \n
S * *        which is called instead of the default functions \n
S *          in order to your HOST dependent functions can access to @ref _WIZCHIP_.
S * @note If it is not registered, the default function is called.
S * @sa WIZCHIP_READ(), WIZCHIP_WRITE(), WIZCHIP_READ_BUF(), WIZCHIP_WRITE_BUF()
S * @sa reg_wizchip_cris_cbfunc(), reg_wizchip_cs_cbfunc(), reg_wizchip_bus_cbfunc(), reg_wizchip_spi_cbfunc()
S */
Stypedef struct __WIZCHIP_T__
S{
S
S   uint16_t  if_mode;                     ///< HOST Interface Mode
S   uint8_t   id[6];                       ///< ID of CHIP such as W6100, and so on.
S
S   ///< The set of critical section callback function.
S   struct _CRIS
S   {
S      void (*_e_n_t_e_r_)  (void);        ///< critical section enter
S      void (*_e_x_i_t_) (void);           ///< critical section exit
S   }CRIS;  
S
S   ///< The set of @ref _WIZCHIP_ select control callback function.
S   struct _CS
S   {
S      void (*_s_e_l_e_c_t_)  (void);      ///< @ref _WIZCHIP_ selected
S      void (*_d_e_s_e_l_e_c_t_)(void);    ///< @ref _WIZCHIP_ deselected
S   }CS;  
S
S   ///< The set of interface IO callback function.
S   union _IF
S   {
S      ///< The callback function of @ref _WIZCHIP_IO_MODE_BUS_ such as @ref _WIZCHIP_IO_MODE_BUS_DIR_ and @ref _WIZCHIP_IO_MODE_BUS_INDIR_
S      struct _BUS
S      {
S         iodata_t  (*_read_data)      (uint32_t AddrSel);               ///< Read 1 @ref iodata_t from @ref _WIZCHIP_ through BUS
S         void      (*_write_data)     (uint32_t AddrSel, iodata_t wb);  ///< Write 1 @ref iodata_t to @ref _WIZCHIP_ through BUS
S         void      (*_read_data_buf)  (uint32_t AddrSel, iodata_t* pBuf, datasize_t len, uint8_t addrinc);  ///< Read @ref iodata_t as many as <i>len</i> from @ref _WIZCHIP_ through BUS
S         void      (*_write_data_buf) (uint32_t AddrSel, iodata_t* pBuf, datasize_t len, uint8_t addrinc);  ///< Write @ref iodata_t data as many as <i>len</i> to @ref _WIZCHIP_ through BUS
S      }BUS;
S
S      ///< The callback function of @ref _WIZCHIP_IO_MODE_SPI_ such as @ref _WIZCHIP_IO_MODE_SPI_VDM_ and @ref _WIZCHIP_IO_MODE_SPI_FDM_
S      struct _SPI
S      {
S         uint8_t   (*_read_byte)      (void);                           ///< Read 1 byte data from @ref _WIZCHIP_ through SPI
S         void      (*_write_byte)     (uint8_t wb);                     ///< Write 1 byte data to @ref _WIZCHIP_ through SPI
S         void      (*_read_byte_buf)  (uint8_t* pBuf, datasize_t len);  ///< Read byte data as many as <i>len</i> from @ref _WIZCHIP_ through SPI
S         void      (*_write_byte_buf) (uint8_t* pBuf, datasize_t len);  ///< Write byte data as many as <i>len</i> to @ref _WIZCHIP_ through SPI
S      }SPI;
S   }IF;    
S
S}_WIZCHIP_T_;
S
