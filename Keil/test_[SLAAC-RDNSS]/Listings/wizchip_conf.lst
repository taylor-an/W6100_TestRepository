S/**
S * @ingroup DATA_TYPE
S * @brief Interrupt Kind
S * @details @ref intr_kind can be used as the interrupt bits of @ref _IR_, @ref _SIR_, and @ref _SLIR_,\n
S *          It can be used as the interrupt mask bits of @ref _IMR_, @ref _SIMR_, and @ref _SLIMR_,\n
S *          and also, It can be used as the interrupt clear bits of @ref _IRCLR_, @ref _Sn_IRCLR_, and @ref _SLIRCLR_.
S * @note It can be used with @b OR operation.
S * @sa ctlwizchip(), CW_GET_INTERRUPT, CW_CLR_INTERRUPT, CW_GET_INTRMASK, CW_SET_INTRMASK
S * @sa ctlnetservice(), ctlnetservice_type
S * @sa wizchip_getinterrupt(), wizchip_clrinterrupt(), wizchip_getinterruptmask(), wizchip_setinterruptmask()
S */
Stypedef enum
S{
S   IK_PPPOE_TERMINATED = (1 << 0),     ///< PPPoE Termination Interrupt
S   IK_DEST_UNREACH     = (1 << 1),     ///< ICMPv4 Destination Unreachable Interrupt
S   IK_IP_CONFLICT      = (1 << 2),     ///< IPv4 Address Conflict Interrupt
S   IK_DEST_UNREACH6    = (1 << 4),     ///< ICMPv6 Destination Unreachable Interrupt
S   IK_WOL              = (1 << 7),     ///< WOL magic packet Interrupt
S   IK_NET_ALL          = (0x97),       ///< All Network Interrupt
S   
S   IK_SOCK_0           = (1 << 8),     ///< Socket 0 Interrupt
S   IK_SOCK_1           = (1 << 9),     ///< Socket 1 Interrupt
S   IK_SOCK_2           = (1 << 10),    ///< Socket 2 Interrupt
S   IK_SOCK_3           = (1 << 11),    ///< Socket 3 Interrupt
S   IK_SOCK_4           = (1 << 12),    ///< Socket 4 Interrupt
S   IK_SOCK_5           = (1 << 13),    ///< Socket 5 Interrupt
S   IK_SOCK_6           = (1 << 14),    ///< Socket 6 Interrupt
S   IK_SOCK_7           = (1 << 15),    ///< Socket 7 Interrupt
S   IK_SOCK_ALL         = (0xFF << 8),  ///< All Socket Interrupt
S
S   IK_SOCKL_TOUT       = (1 << 16),    ///< @ref _SLCR_ Timeout Interrupt.\n Refer to @ref ctlnetservice_type.
S   IK_SOCKL_ARP4       = (1 << 17),    ///< @ref _SLCR_ APR4 Interrupt.\n Refer to @ref CNS_ARP.
S   IK_SOCKL_PING4      = (1 << 18),    ///< @ref _SLCR_ PING4 Interrupt.\n Refer to @ref CNS_PING.
S   IK_SOCKL_ARP6       = (1 << 19),    ///< @ref _SLCR_ ARP6 Interrupt.\n Refer to @ref CNS_ARP.
S   IK_SOCKL_PING6      = (1 << 20),    ///< @ref _SLCR_ PING6 Interrupt.\n Refer to @ref CNS_PING.
S   IK_SOCKL_NS         = (1 << 21),    ///< @ref _SLCR_ NS Interrupt.\n Refer to @ref CNS_DAD.
S   IK_SOCKL_RS         = (1 << 22),    ///< @ref _SLCR_ RS Interrupt.\n Refer to @ref CNS_SLAAC.
S   IK_SOCKL_RA         = (1 << 23),    ///< @ref _SLCR_ RA Interrupt.\n Refer to @ref CNS_GET_PREFIX.
S   IK_SOCKL_ALL        = (0xFF << 16), ///< @ref _SLCR_ All Interrupt
S
S   IK_INT_ALL          = (0x00FFFF97)   ///< All Interrupt
S}intr_kind;
