N * @sa _SYCR1_, SYCR1_CLKSEL, SYCR1_CLKSEL_25M
N * @sa getSYCR1(), setSYCR1(), getSYSR(), getCHPLCKR(), setCHIPLCKR(), CHIPLOCK(), CHIPUNLOCK()
N */
N#define SYCR1_CLKSEL_100M    0
