S/**
S * @ingroup DATA_TYPE
S * @brief Ethernet PHY operation mode configuration
S * @details @ref wiz_PhyConf is a structure type to configure or \n
S *          indicate the PHY operation mode such as auto-negotiation, speed, and duplex.\n
S * @note If @ref _PHY_IO_MODE_ is defined with @ref _PHY_IO_MODE_PHYCR_,\n
S *       the PHY operation mode is configured by @ref _PHYCR0_ and is indicated by @ref _PHYSR_.\n
S *       Otherwise, the PHY operation mode is configured or indicated by two MDC/MDIO signal of MII.
S * @note In order to apply @ref wiz_PhyConf to the integrated Ethernet PHY, \n
S *       It needs to reset the PHY by calling @ref ctlwizchip(@ref CW_RESET_PHY)
S * @sa ctlwizchip(), CW_SET_PHYCONF, CW_GET_PHYCONF, CW_RESET_PHY
S * @sa wizphy_setphyconf(), wizphy_getphyconf(), wizphy_reset()
S */
Stypedef struct wiz_PhyConf_t
S{
S      uint8_t mode;     ///< set by @ref PHY_MODE_MANUAL ,@ref PHY_MODE_AUTONEGO, or @ref PHY_MODE_TE
S      uint8_t speed;    ///< set by @ref PHY_SPEED_10 or @ref PHY_SPEED_100
S      uint8_t duplex;   ///< set by @ref PHY_DUPLEX_HALF @ref PHY_DUPLEX_FULL 
S}wiz_PhyConf;
S
S/**
S * @ingroup DATA_TYPE
S * @brief IP Address Configuration Mode
S * @details @ref ipconf_mode can be used to save the DHCP mode running on your system.
S * @sa ctlnetwork(), CN_SET_NETINFO, CN_GET_NETINFO
S * @sa wizchip_setnetinfo(), wizchip_getnetinfo(), wiz_NetInfo
S */
Stypedef enum
S{
S   NETINFO_NONE       = 0x00,    ///< No use DHCP
S   NETINFO_STATIC_V4  = 0x01,    ///< Static IPv4 configuration by manually.
S   NETINFO_STATIC_V6  = 0x02,    ///< Static IPv6 configuration by manually.
S   NETINFO_STATIC_ALL = 0x03,    ///< Static IPv4 and IPv6 configuration by manually.
S   NETINFO_SLAAC_V6   = 0x04,    ///< Stateless Adders Auto Configuration for IPv6
S   NETINFO_DHCP_V4    = 0x10,    ///< Dynamic IPv4 configuration from a DHCP sever
S   NETINFO_DHCP_V6    = 0x20,    ///< Dynamic IPv6 configuration from a DHCP sever
S   NETINFO_DHCP_ALL   = 0x30     ///< Dynamic IPv4 and IPv6 configuration from a DHCP sever
S}ipconf_mode;
S
S/**
S * @ingroup DATA_TYPE
S * @brief Network Information for @ref _WIZCHIP_
S * @details @ref wiz_NetInfo is a structure type to configure or indicate the network information of @ref _WIZCHIP_.
S * @sa ctlnetwork(), CN_SET_NETINFO, CN_GET_NETINFO
S * @sa wizchip_setnetinfo(), wizchip_getnetinfo()
S */
Stypedef struct wiz_NetInfo_t
S{
S   uint8_t     mac[6];    ///< Source Hardware Address
S   uint8_t     ip[4];     ///< Source IPv4 Address
S   uint8_t     sn[4];     ///< Subnet Mask value
S   uint8_t     gw[4];     ///< Gateway IPv4 Address
S   uint8_t     lla[16];   ///< Source Link Local Address
S   uint8_t     gua[16];   ///< Source Global Unicast Address
S   uint8_t     sn6[16];   ///< IPv6 Prefix
S   uint8_t     gw6[16];   ///< Gateway IPv6 Address
S   uint8_t     dns[4];    ///< DNS server IPv4 Address
S   uint8_t     dns6[16];  ///< DNS server IPv6 Address
S   ipconf_mode ipmode;    ///< IP Configuration Mode
S}wiz_NetInfo;
S
S/**
S * @ingroup DATA_TYPE
S * @brief Network mode Configuration
S * @details @ref netmode_type includes the network mode control function such as ping, TCP/RST block and etc.
S * @sa ctlnetwork(), CN_SET_NETMODE, CN_GET_NETMODE
S */
Stypedef enum
S{
S   // NETMR Bit Values
S   NM_IPB_V4            = (1 << 0),      ///< IPv4 Packet Block
S   NM_IPB_V6            = (1 << 1),      ///< IPv6 Packet Block
S   NM_WOL               = (1 << 2),      ///< Wake On Lan(WOL) Mode
S   NM_PB6_MULTI         = (1 << 4),      ///< PING6 request from multicasting group address Block
S   NM_PB6_ALLNODE       = (1 << 5),      ///< PING6 request from all-node multicasting address Block
S   NM_MR_MASK           = (0x37),        ///< @ref _NETMR_ Mask value
S
S   // NETMR2 Bit Values
S   NM_PPPoE             = (1 << 8),      ///< PPPoE Mode
S   NM_DHA_SELECT        = (1 << 15),     ///< Destination Hardware Address Select
S   NM_MR2_MASK          = (0x09<<8),     ///< @ref _NETMR2_ Mask value
S
S  //NET4MR Bit Values
S   NM_PB4_ALL           = (1 << 16),     ///< All PING4 request Block
S   NM_TRSTB_V4          = (1 << 17),     ///< TCP RST packet for IPv4 Send Block
S   NM_PARP_V4           = (1 << 18),     ///< ARP request for IPv4 before PINGv4 Replay
S   NM_UNRB_V4           = (1 << 19),     ///< Unreachable Destination for IPv4 Block
S   NM_NET4_MASK         = (0x0F << 16),  ///< @ref _NET4MR_ Mask value
S
S  //NET4MR Bit Values
S   NM_PB6_ALL           = (1 << 24),     ///< All PING6 request Block
S   NM_TRSTB_V6          = (1 << 25),     ///< TCP RST packet for IPv6 Send Block
S   NM_PARP_V6           = (1 << 26),     ///< ARP request for IPv6 before PINGv4 Replay
S   NM_UNRB_V6           = (1 << 27),     ///< Unreachable Destination for IPv6 Block
S   NM_NET6_MASK         = (0x0F << 24),  ///< @ref _NET6MR_ Mask value
S
S   NM_MASK_ALL          = (0x0F0F0937)   ///< @ref netmode_type all mask value
S}netmode_type;
S
S/**
S * @ingroup DATA_TYPE
S * @brief Network Timeout for @ref _WIZCHIP_
