N   struct _CS
N   {
N      void (*_s_e_l_e_c_t_)  (void);      ///< @ref _WIZCHIP_ selected
N      void (*_d_e_s_e_l_e_c_t_)(void);    ///< @ref _WIZCHIP_ deselected
